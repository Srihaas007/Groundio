{
  "name": "groundio",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:web": "cd web-app && npm run dev",
    "start:mobile": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "build": "expo export",
    "build:web": "cd web-app && npm run build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "preview": "expo start --tunnel",
    "eject": "expo eject",
    "doctor": "expo doctor",
    "install:clean": "rm -rf node_modules && npm install",
    "postinstall": "npx expo install --fix",
    "docker:dev": "docker-compose up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "docker:build": "docker build -t groundio:latest .",
    "k8s:deploy": "bash scripts/deploy.sh",
    "k8s:deploy:win": "scripts\\deploy.bat"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.10",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/geolocation": "^3.3.0",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/auth": "^20.4.0",
    "@react-native-firebase/storage": "^20.4.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/material-top-tabs": "^6.6.14",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.4.1",
    "date-fns": "^3.6.0",
    "expo": "~51.0.39",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-device": "6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-font": "~12.0.9",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-modules-core": "~1.12.23",
    "expo-notifications": "~0.28.19",
    "expo-permissions": "^14.4.0",
    "expo-router": "~3.5.24",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "firebase": "^10.13.0",
    "react": "^18.2.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-document-picker": "^9.3.1",
